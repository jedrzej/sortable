{
  "name": "jedrzej/sortable",
  "description": "Sortable trait for Laravel's Eloquent models - sort your models using request parameters",
  "keywords": [
    "laravel",
    "sort",
    "sortable",
    "eloquent",
    "model",
    "api",
    "request"
  ],
  "homepage": "https://github.com/jedrzej/sortable",
  "license": "MIT",
  "authors": [
    {
      "name": "Jędrzej Kuryło",
      "email": "jedrzej.kurylo@gmail.com"
    }
  ],
  "require": {
    "illuminate/database": "*",
    "illuminate/support": "*"
  },
  "require-dev": {
    "codeception/codeception": "2.*",
    "codeception/specify": "*",
    "codeception/assert-throws": "*",
    "mockery/mockery": "*"
  },
  "suggest": {
    "jedrzej/searchable": "Allows filtering your models using request parameters",
    "jedrzej/withable": "Allows eager loading of relations using request parameters"
  },
  "autoload": {
    "psr-4": {
      "Jedrzej\\": "src/Jedrzej/",
      "Illuminate\\": "vendor/laravel/framework/src/Illuminate/"
    }
  }
}
